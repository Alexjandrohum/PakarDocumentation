package com.scpakar.scpakarweb.dto;

import java.io.Serializable;

/**
 *
 * @author pablo.martinez
 */
public class TotalEstatusPedidoDTO implements Serializable {

    private int totalPedidos;
    private int totalEnviados;
    private int totalRecibidos;
    private int totalHistorico;

    public TotalEstatusPedidoDTO() {
        this.totalPedidos = 0;
        this.totalEnviados = 0;
        this.totalRecibidos = 0;
        this.totalHistorico = 0;
    }

    public int getTotalPedidos() {
        return totalPedidos;
    }

    public void setTotalPedidos(int totalPedidos) {
        this.totalPedidos = totalPedidos;
    }

    public int getTotalEnviados() {
        return totalEnviados;
    }

    public void setTotalEnviados(int totalEnviados) {
        this.totalEnviados = totalEnviados;
    }

    public int getTotalRecibidos() {
        return totalRecibidos;
    }

    public void setTotalRecibidos(int totalRecibidos) {
        this.totalRecibidos = totalRecibidos;
    }

    public int getTotalHistorico() {
        return totalHistorico;
    }

    public void setTotalHistorico(int totalHistorico) {
        this.totalHistorico = totalHistorico;
    }
}
