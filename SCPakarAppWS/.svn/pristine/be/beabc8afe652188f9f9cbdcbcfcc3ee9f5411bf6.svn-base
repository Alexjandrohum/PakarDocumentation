package com.scpakar.scpakarappws.dto;

import java.io.Serializable;

public class ChatMensajeDTO implements Serializable {
    private ChatOrigen origen;
    private ChatEstatus estatus;
    private String mensaje;
    private String fecha;
    private String hora;
    private String tiempo;

    public ChatMensajeDTO() {
    }

    public String getTiempo() {
        return tiempo;
    }

    public void setTiempo(String tiempo) {
        this.tiempo = tiempo;
    }

    public ChatEstatus getEstatus() {
        return estatus;
    }

    public void setEstatus(ChatEstatus estatus) {
        this.estatus = estatus;
    }

    public String getHora() {
        return hora;
    }

    public void setHora(String hora) {
        this.hora = hora;
    }

    public ChatOrigen getOrigen() {
        return origen;
    }

    public void setOrigen(ChatOrigen origen) {
        this.origen = origen;
    }

    public String getMensaje() {
        return mensaje;
    }

    public void setMensaje(String mensaje) {
        this.mensaje = mensaje;
    }

    public String getFecha() {
        return fecha;
    }

    public void setFecha(String fecha) {
        this.fecha = fecha;
    }

}
